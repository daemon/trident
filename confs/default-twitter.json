    "seed": 0,
    "training_pct": 80,
    "dev_pct": 10,
    "test_pct": 10,
    "dataset_file": "tweets.csv",
    "batch_size": 64,
    "lr": 0.001,
    "clip_grad_norm": 20,
    "epochs": 16,
    "resume": "twitter.pt",
    "test": false,
    "output_file": "twitter.pt",
    "weight_decay": 1E-6
}
